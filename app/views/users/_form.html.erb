<%= render 'layouts/errors', :object => @user %>

<%= form_for User.new do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "Sign Up"%>
<% end %>
